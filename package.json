  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@types/webextension-polyfill": "latest",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-plugin-format": "^1.0.1",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.0.6"
