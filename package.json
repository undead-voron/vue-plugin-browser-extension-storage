  "version": "0.0.12",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "watch": "vite build --watch --mode development --minify false",
    "dev": "vite",
    "test": "vitest --run --globals --config vitest.config.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "ci:publish": "pnpm publish --no-git-checks",
    "ci:bump-version": "pnpm version minor",
    "ci:patch-version": "pnpm version patch",
    "ci:bump-version-major": "pnpm version major"
  },
  "keywords": [],
  "repository": {
    "url": "https://github.com/undead-voron/vue-plugin-browser-extension-storage",
    "type": "git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "vue": "^3",
